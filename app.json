{
  "expo": {
    "name": "pak-style",
    "slug": "pak-style",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pak1.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/pak2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zubi0258.pakstyle"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/pak1.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zubi0258.pakstyle",
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "152fba50-45e9-4d3e-970c-4eabd973c59a"
      }
    }
  }
}
